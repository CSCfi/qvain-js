    "json-pointer": "^0.6.0",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "mocha": "^5.2.0"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
